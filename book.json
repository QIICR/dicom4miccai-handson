{
  "plugins": [
    "anchorjs",
    "-lunr",
    "-search",
    "search-plus",
    "etoc",
    "ga"
  ],
  "pluginsConfig": {
    "anchorjs": {
      "placement": "right",
      "visible": "hover"
    },
    "page-toc": {
      "position": "before-first"
    },
    "etoc": {
      "mindepth": 2,
      "maxdepth": 4,
      "notoc": false
    },
    "ga": {
      "token": "UA-44852938-10"
    }
  }
}
